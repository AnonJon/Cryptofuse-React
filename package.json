    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.7",
    "nodemon": "^1.19.4",
    "popper.js": "^1.16.0"
